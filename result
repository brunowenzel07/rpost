 pageTracker._setCustomVar (2, 'Status', ($.cookie('PermRpLogin') && $.cookie('RP_UID') && $.cookie('PermRpPackage') ? 'Logged in': 'Not logged'), 2);
 pageTracker._setCustomVar (3, 'CustomerID', $.cookie('RP_UID'), 3);
 pageTracker._setCustomVar (4, 'Tracker', $.cookie('__t'), 1);
 pageTracker._trackPageview();
 rpAnalytics.trackPreloadedUrls();
 } catch(err) {}
</script> </body>
